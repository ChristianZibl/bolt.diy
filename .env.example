# Configuration AWS Bedrock
# Remplissez avec vos informations AWS pour accéder aux services Bedrock
AWS_BEDROCK_CONFIG={"region": "us-east-1", "accessKeyId": "yourAccessKeyId", "secretAccessKey": "yourSecretAccessKey"}

# Niveau de journalisation pour le développement local
VITE_LOG_LEVEL=debug

# Contexte par défaut pour les modèles Qwen2.5-coder:32b
# Ajustez en fonction de votre capacité mémoire (VRAM)
# Exemples :
# DEFAULT_NUM_CTX=32768  # Consomme 36GB de VRAM
# DEFAULT_NUM_CTX=24576  # Consomme 32GB de VRAM
# DEFAULT_NUM_CTX=12288  # Consomme 26GB de VRAM
# DEFAULT_NUM_CTX=6144   # Consomme 24GB de VRAM
DEFAULT_NUM_CTX=
